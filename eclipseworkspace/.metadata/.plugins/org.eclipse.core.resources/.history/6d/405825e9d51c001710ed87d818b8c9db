package com.justin.RentalSystem;

public class CarReporter {

	public String printAll(Car car) {
		System.out.println("This is the make " + car.getMake() + " This is the Model " + car.getModel()
				+ " This is the year " + car.getYear() + " this is how many miles " + car.getMiles());
		return ("it worked");
	}

	public String printAll(House house) {
		System.out.println("I live at " + house.getAddress() + " it has " + house.getSquareFeet()
				+ " square feet and is " + house.getColor() + " in color " + house.getFence() + "i got fence.");
		return ("it worked for house");
	}

	public static void main(String[] args) {
		CarReporter carReporter = new CarReporter();
		Car car = new Car();
		House house = new House();
		
		carReporter.printAll(car);
		carReporter.printAll(house);
	}
}
