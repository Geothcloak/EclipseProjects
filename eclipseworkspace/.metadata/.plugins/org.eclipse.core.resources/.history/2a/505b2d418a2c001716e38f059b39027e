package com.justin.fileio;

public class Rogue extends PlayerCharacter{
	private static final String ROGUE = "Rogue";
	private String classType;
	private String feat;
	
	public Rogue(String name, Stats stats, String classType, String feat) {
		super(name, stats);
		this.classType = ROGUE;
		this.feat = feat;
	}
	
	@Override
	public String getClassType() {
		return classType;
	}

	@Override
	public String getFeat() {
		return feat;
	}

}
