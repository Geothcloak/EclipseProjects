package com.justin.shippingSystem;

public class Dimentions {
	private double dimentionX;
	private double dimentionY;
	private double dimentionZ;
	public Dimentions(double dimentionX, double dimentionY, double dimentionZ){
		this.dimentionX = dimentionX;
		this.dimentionY = dimentionY;
		this.dimentionZ = dimentionZ;
	}
	public double getDimentionX() {
		return dimentionX;
	}
	public double getDimentionY() {
		return dimentionY;
	}
	public double getDimentionZ() {
		return dimentionZ;
	}
	public double calcVolume(){
		
		return dimentionX * dimentionY * dimentionZ;
	}
	public double calcWeightPerFoot(Weight weight){
		return 1;
	}
}
