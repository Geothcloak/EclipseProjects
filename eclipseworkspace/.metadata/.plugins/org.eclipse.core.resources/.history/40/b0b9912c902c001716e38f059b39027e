package com.justin.fileio;

public class DestructiveSpell {
	private String name;
	private String element;
	private double damageFactor;
	
	public DestructiveSpell(String name, String element, double damageFactor) {
		super();
		this.name = name;
		this.element = element;
		this.damageFactor = damageFactor;
	}
	
	
}
