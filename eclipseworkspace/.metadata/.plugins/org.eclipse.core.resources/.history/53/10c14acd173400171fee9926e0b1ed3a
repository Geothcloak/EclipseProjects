package com.justin.dcompany;

import java.util.Random;

public class RewardSystem {
	private double maxReward = 2;
	private double minReward = .5;
	private double randomAmount = 0;
	public double PaycheckReward(double payCheck){
		Random rand = new Random();
		randomAmount = rand.nextDouble() * maxReward + minReward;
	//	payCheck * 
		return 0;
	}
}
