package com.justin.totemBiz;

public class Totem {
	float height;
	String name;
	String material;
	int totalSpirits;
	
	public Totem(float height, String name, String material, int totalSpirits){
		this.height = height;
		this.name = name;
		this.material = material;
		this.totalSpirits = totalSpirits;
	}
	
}
