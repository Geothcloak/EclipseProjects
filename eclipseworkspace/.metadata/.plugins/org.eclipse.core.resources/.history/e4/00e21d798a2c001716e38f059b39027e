package com.justin.fileio;

public class Cleric extends PlayerCharacter{
	private String classType;
	private String feat;
	
	public Cleric(String name, Stats stats, String classType, String feat) {
		super(name, stats);
		this.classType = classType;
		this.feat = feat;
	}
	@Override
	public String getClassType() {
		return classType;
	}

	@Override
	public String getFeat() {
		return feat;
	}
	

}
