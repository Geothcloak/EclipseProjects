package com.justin.shippingSystem;

public class Crate extends Parcel{
	private Dimentions dimention;
	private Material material;
	private Weight weight;
	public Crate(String id, Address address, Dimentions dimention, Material material, Weight weight){
		super(id, address);
		this.dimention = dimention;
		this.material = material;
		this.weight = weight;
	}
	@Override
	public String whatAmI(){
		return "Crate";
	}
	
	@Override
	public String specialInformation(){
		return ("The dimentions are " + dimention.getDimentionX() + "x" + dimention.getDimentionY() + "x" + dimention.getDimentionZ()
		+ "its weight in lbs " +  weight.getLbs() + " its material is " + material.getName());
		
	}
	
}